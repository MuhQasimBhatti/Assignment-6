#include<iostream>
using namespace std;

int func(int *A,int a)
{
	int c = A[0];
	for (int i = 0; i < a; i++)
		if (A[i] < c)
			c = A[i];
	return c;
}

int main()
{
	cout << "\t\tQuestion no 6" << endl;
	int A[5];
	cout << "Enter 5 numbers" << endl;
	for (int i = 0; i < 5; i++)
		cin >> A[i];
	func(A, 5);
	cout<< func(A, 5);
}
