#include <iostream>
#include<iomanip>
using namespace std;

int display(int a[5][5])
{
	for (int i = 0; i < 5; i++)
	{
		for (int j = 0; j < 5; j++)
			cout <<setw(3) << a[i][j] ;
		cout << endl;
	}
	return 0;
}

int time2(int a)
{
	a = a * 2;
	return a;
}

int main()
{
	cout << "\t\tQuestion no 8" << endl;
	int arr[5][5];
	int arr2[5][5];
	cout << "Enter matrix" << endl;
	for (int i = 0; i < 5; i++)
	{
		for (int j = 0; j < 5; j++)
		{
			cin >> arr[i][j];
			arr2[i][j] = time2(arr[i][j]);
		}
	}
	cout << "\nYou enter " << endl;
	display(arr);
	cout << "\n2x values" << endl;
	display(arr2);
	
}
